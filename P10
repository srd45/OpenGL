import cv2

image_path = 'photo.jpg'
img =cv2.imread(image_path)

if img is None:
    print(f"Error: Unable to load the image '{image_path}'. Please check the file path")

blurred_img_gaussian = cv2.GaussianBlur(img, (11,11), 0)
blurred_img_median = cv2.medianBlur(img, 5)

cv2.imshow("Original Image", img)
cv2.imshow("Gaussian-blurred Image", blurred_img_gaussian)
cv2.imshow("Median-blurred Image", blurred_img_median)
cv2.waitKey(0)
cv2.destroyAllWindows()
